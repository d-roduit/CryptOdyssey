{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console":"off",
        "indent": ["error", 4],
        "quote-props": ["error", "consistent"],
        "import/extensions": ["error", "ignorePackages"]
    }
}
